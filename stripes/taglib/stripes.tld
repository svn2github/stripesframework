        <attribute><description>@value@</description><name>value</name><required>false</required><rtexprvalue>true</rtexprvalue></attribute>
        <attribute><description>@disabled@</description><name>disabled</name><required>false</required><rtexprvalue>true</rtexprvalue></attribute>
        <!-- End: Standard Form Input   attributes -->

